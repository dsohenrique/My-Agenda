CREATE DATABASE portifolio;

USE portifolio;

CREATE TABLE agenda(
   codigo int primary key auto_increment,
   data date,
   tarefa varchar(50)
);

INSERT INTO agenda(data, tarefa) value ('2019-04-04', 'Devolver o livro na Biblioteca');
